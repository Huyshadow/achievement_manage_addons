<odoo>
<data>

    <record model="ir.ui.view" id="achievement_management.list_user">
      <field name="name">achievement_management list</field>
      <field name="model">database_manage.user</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="mssv"/>
          <field name="email"/>
          <field name="role"/>
        </tree>
      </field>
    </record>

    <menuitem 
      name="Information" 
      id="achievement_management.info" 
      action="achievement_management.action_window_depart" 
      web_icon="code_backend_theme,static/src/img/icons/Recruitment.png"
    />
</data>
</odoo>