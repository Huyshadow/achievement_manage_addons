<odoo>
    <record id="achievement_statistic_view_tree" model="ir.ui.view">
        <field name="name">graph.view.achievement.statis.tree</field>
        <field name="model">achievement.department.statistic</field>
        <field name="arch" type="xml">
            <tree create="false" export_xlsx="false" delete='false' edit="false"
                js_class="disappear_check_box">
                <field name="depart_name" string="Tên đơn vị" class="custom_column" />
                <field name="num_of_submit" string="Số hồ sơ đã nộp" />
                <field name="num_of_accept" string="Số hồ sơ đã duyệt" />
            </tree>
        </field>
    </record>
</odoo>