<odoo>
    <record id="achievement_statistic_view_search" model="ir.ui.view">
        <field name="name">graph.view.achievement.statis.search</field>
        <field name="model">achievement.graph.report</field>
        <field name="arch" type="xml">
            <search>
                <field name="achievement_id" />
            </search>
        </field>
    </record>

    <record id="achievement_statistic_view_graph" model="ir.ui.view">
        <field name="name">graph.view.achievement.statis.graph</field>
        <field name="model">achievement.graph.report</field>
        <field name="arch" type="xml">
            <graph string="Đồ thị mô tả" disable_linking="1" stacked="False" type="bar"
                order="desc">
                <field name="depart_name" string="Danh sách đơn vị" />
                <field name="type" />
                <field name="display_nums" type="measure" />
            </graph>
        </field>
    </record>
</odoo>