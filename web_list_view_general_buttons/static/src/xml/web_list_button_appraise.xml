<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="ListView.AppraiseButtons">
        <t t-foreach="buttons" t-as="button">
            <button
                type="button"
                class="btn btn-primary btn-custom o_appraise_button"
                t-att-action="button.action"
                t-att-model="button.model"
            >
                <t t-esc="button.name" />
            </button>
        </t>
    </t>
</templates>