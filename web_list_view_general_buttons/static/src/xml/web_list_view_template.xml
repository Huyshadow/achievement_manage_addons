<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="ListView.GeneralButtons">
        <!-- <t t-if="achievement.user.list.user_approve === False"> -->
        <t t-foreach="buttons" t-as="button">
            <button
                type="button"
                class="btn btn-primary btn-custom o_general_button"
                t-att-action="button.action"
                t-att-model="button.model"
            >
                <t t-esc="button.name" />
            </button>
        </t>
        <!-- </t> -->
    </t>
</templates>